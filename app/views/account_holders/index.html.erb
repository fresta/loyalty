<p id="notice"><%= notice %></p>

<h1>Account Holders</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @account_holders.each do |account_holder| %>
      <tr>
        <td><%= link_to 'Show', account_holder %></td>
        <td><%= link_to 'Edit', edit_account_holder_path(account_holder) %></td>
        <td><%= link_to 'Destroy', account_holder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account Holder', new_account_holder_path %>
